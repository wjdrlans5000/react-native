{
  "expo": {
    "name": "OpenLayersWebViewExpo",
    "slug": "OpenLayersWebViewExpo",
    "platforms": [
      "ios",
      "android",
      "web"
    ],
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier" : "com.forcewave.OpenLayersWebViewExpo"
    },
    "packagerOpts" : {
      "sourceExts" : ["ts","tsx","js"],
      "transformer" : "node_modules/react-native-typescript-transformer/index.js"
    }
  }
}
